{
    "workspace": {
        "library": [
            "annotations/love2d/library"
        ]
    },
    "runtime.version": "LuaJIT",
    "runtime.special": {
        "love.filesystem.load": "loadfile"
    },
    "workspace.checkThirdParty": false,
    "diagnostics.globals": [
        "app",
        "assert_type",
        "class",
        "config",
        "get_class_name_of",
        "get_class_of",
        "is_instance_of"
    ],
    "diagnostics.disable": [
        "param-type-mismatch"
    ]
}